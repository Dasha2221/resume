<div>
    <main>
        <section>
            <div class="row-12">
                {{#with navigation}}
                {{#each links}}
                <a href="{{href}}" class="btn btn-link col-2">
                    {{text}}
                </a>
                {{/each}}
                {{#with button}}
                <a href="{{href}}" class="btn btn-primary col-2">
                    {{text}}
                </a>
                {{/with}}
                {{/with}}

                <hr>

                <div>
                    {{#with header}}
                    <h1>{{title}}</h1>
                    <p>{{description}}</p>
                    {{#with button}}
                    <a href="{{link}}" class="btn btn-primary">
                        {{text}}
                    </a>
                    {{/with}}

                    {{/with}}
                    <br>
                    <br>
                    <hr>
                    <div>
                        {{#with city}}
                        <h2>{{title}}</h2>

                        {{#each cards}}
                        <div class="col-5">
                            <div class="card">
                                {{#with picture}}
                                <img src={{src}} alt={{alt}}>

                                {{/with}}
                                <div class="card-body">
                                    <h3 class="card-title"> {{title}}</h3>
                                    <p>{{description}}</p>
                                    {{#with button}}
                                    <a href="{{href}}" class="btn btn-primary col-2">
                                        {{text}}
                                    </a>
                                    {{/with}}

                                </div>
                            </div>
                        </div>
                        {{/each}}

                        {{/with}}
                    </div>

                    <br>
                    <hr>
                    <div>
                        {{#with winner}}
                        <h2>{{title}}</h2>
                        <hr>
                        {{#with table}}
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>{{head}}</th>
                                </tr>
                            </thead>
                            {{#each user}}
                            <tbody>
                                <tr>
                                    <th>{{place}}</th>
                                    <td>{{name}}</td>
                                    <td>{{surname}}</td>
                                    <td>{{nickname}}</td>
                                </tr>
                            </tbody>
                            {{/each}}
                        </table>
                        {{/with}}
                        {{/with}}
                    </div>
                    <br>
                    <br>

                    <div>
                        {{#with gallery}}
                        <h2 class="mt-4 mb-4">{{title}}</h2>
                        <div class="row g-2">
                            {{#each cards}}
                            <section class="col-4">
                                <div class="card ">
                                    <div class="card-header">
                                        <div>{{name}}</div>
                                    </div>


                                    <ul class="list-group list-group-flush">
                                        {{#each items}}
                                        <li class="list-group-item">{{this}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </section>
                            {{/each}}
                        </div>
                        {{/with}}
                    </div>

                    <hr>
                    <br>
                    <div>
                        {{#with relaxion}}
                        <p class="display-4">{{title}}</p>
                        <div class="row g-2">
                            {{#each cards}}
                            <section class="col-5">
                                <div class="card text-bg-secondary">
                                    <div class="card-header">
                                        <div>{{header}}</div>
                                    </div>

                                    <div class="card-body">
                                        <div class="card-title">
                                            <h6>{{title}}</h6>
                                            <p>{{description}}</p>
                                        </div>

                                    </div>
                                </div>
                            </section>
                            {{/each}}

                        </div>
                        {{/with}}
                    </div>
                    <br>
                    <hr>
                    <div>
                        {{#with rest}}
                        <div class="row g-2">
                            {{#each cards}}
                            <section class="col-6">
                                <div class="card text-center">
                                    <div class="card-header">
                                        <div>{{header}}</div>
                                    </div>

                                    <div class="card-body">
                                        <div class="card-title">
                                            <h6>{{title}}</h6>
                                            <p>{{description}}</p>
                                        </div>
                                        <button class="btn btn-primary">{{button}}</button>

                                    </div>
                                    <div class="card-footer">
                                        <div>{{footer}}</div>
                                    </div>
                                </div>
                            </section>
                            {{/each}}

                        </div>
                        {{/with}}
                    </div>

                    <br>
                    <br>
                    <hr>

                    <div>
                        {{#with proposition}}
                        <p class="display-2">{{title}}</p>
                        <br>
                        <br>
                        <div class="row g-2">
                            {{#each card}}
                            <section class="col-5">
                                <div class="card text-bg-warning">
                                    <div class="card-header">
                                        <div>{{header}}</div>
                                    </div>

                                    <div class="card-body">
                                        <div class="card-title">
                                            <h6>{{title}}</h6>
                                            <p>{{description}}</p>
                                        </div>

                                    </div>
                                </div>
                            </section>
                            {{/each}}

                        </div>
                        {{/with}}
                    </div>
                    <br>
                    <hr>
                    <div>

                        {{#with footer}}
                        <h5>{{title}}</h5>
                        <div class="row  g-2">
                            <section class="col-3">
                                {{#each navigation}}

                                <ul>
                                    <a href="{{href}}" class="btn btn-link ">
                                        {{text}}
                                    </a>
                                </ul>
                                {{/each}}
                                {{#each socialMedia}}
                                <ul>
                                    <a href="{{link}}" class="btn btn-outline-dark ">
                                        {{name}}
                                    </a>
                                </ul>
                                {{/each}}
                                {{#each partners}}
                                <ul>
                                    <a href="{{link}}" class="btn btn-outline-info ">
                                        {{name}}
                                    </a>
                                </ul>
                                {{/each}}
                            </section>
                        </div>
                        {{/with}}

                    </div>
                </div>
            </div>
</div>
</section>
</main>
</div>