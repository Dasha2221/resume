<div class="row m-2">
    {{#with main}}
    <main>
        <section class="col">
            {{> title data="My educationss" big=true}}
            <ul class="ps-5">
                {{#each educations}}
                <li>
                    {{name}}
                    {{#unless isEnd}} (ще в процесі) {{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>
        <section>
            {{> title data="My certificates"}}
            <ul class="ps-5">
                {{#each certificates}}
                <li>
                    {{name}}
                    ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>